{
    "type": "scaffold",
    "args": {
        "backgroundColor": "#fff0faff",
        "body": {
            "type": "center",
            "args": {
                "child": {
                    "type": "single_child_scroll_view",
                    "args": {
                        "child": {
                            "type": "padding",
                            "args": {
                                "padding": {
                                    "bottom": 20.0,
                                    "left": 20.0,
                                    "right": 20.0,
                                    "top": 20.0
                                },
                                "child": {
                                    "type": "container",
                                    "args": {
                                        "decoration": {
                                            "border": {
                                                "bottom": {
                                                    "color": "#ffdbeafe",
                                                    "strokeAlign": -1.0,
                                                    "style": "solid",
                                                    "width": 2.0
                                                },
                                                "left": {
                                                    "color": "#ffdbeafe",
                                                    "strokeAlign": -1.0,
                                                    "style": "solid",
                                                    "width": 2.0
                                                },
                                                "right": {
                                                    "color": "#ffdbeafe",
                                                    "strokeAlign": -1.0,
                                                    "style": "solid",
                                                    "width": 2.0
                                                },
                                                "top": {
                                                    "color": "#ffdbeafe",
                                                    "strokeAlign": -1.0,
                                                    "style": "solid",
                                                    "width": 2.0
                                                }
                                            },
                                            "borderRadius": {
                                                "bottomLeft": {
                                                    "type": "elliptical",
                                                    "x": 10.0,
                                                    "y": 10.0
                                                },
                                                "bottomRight": {
                                                    "type": "elliptical",
                                                    "x": 10.0,
                                                    "y": 10.0
                                                },
                                                "topLeft": {
                                                    "type": "elliptical",
                                                    "x": 10.0,
                                                    "y": 10.0
                                                },
                                                "topRight": {
                                                    "type": "elliptical",
                                                    "x": 10.0,
                                                    "y": 10.0
                                                },
                                                "type": "only"
                                            },
                                            "boxShadow": [
                                                {
                                                    "blurRadius": 5.0,
                                                    "blurStyle": "normal",
                                                    "color": "#42000000",
                                                    "offset": {
                                                        "dx": 0.0,
                                                        "dy": 4.0
                                                    },
                                                    "spreadRadius": 2.0
                                                }
                                            ],
                                            "color": "#fff0faff",
                                            "shape": "rectangle"
                                        },
                                        "height": 180.0,
                                        "padding": {
                                            "bottom": 10.0,
                                            "left": 10.0,
                                            "right": 10.0,
                                            "top": 10.0
                                        },
                                        "width": "${null}",
                                        "child": {
                                            "type": "column",
                                            "args": {
                                                "mainAxisAlignment": "center",
                                                "children": [
                                                    {
                                                        "type": "text_button",
                                                        "args": {
                                                            "onPressed": "${navigatePage('test_page')}",
                                                            "style": {
                                                                "backgroundColor": {
                                                                    "disabled": "#fff44336",
                                                                    "dragged": "#fff44336",
                                                                    "empty": "#fff44336",
                                                                    "error": "#fff44336",
                                                                    "focused": "#fff44336",
                                                                    "hovered": "#fff44336",
                                                                    "pressed": "#fff44336",
                                                                    "scrolledUnder": "#fff44336",
                                                                    "selected": "#fff44336"
                                                                }
                                                            },
                                                            "child": {
                                                                "type": "text",
                                                                "args": {
                                                                    "text": "ir proxima pagina"
                                                                }
                                                            }
                                                        }
                                                    },
                                                    {
                                                        "type": "sized_box",
                                                        "args": {
                                                            "height": 20.0
                                                        }
                                                    },
                                                    {
                                                        "type": "text_button",
                                                        "args": {
                                                            "onPressed": "${navigate_pop(false)}",
                                                            "style": {
                                                                "backgroundColor": {
                                                                    "disabled": "#fff44336",
                                                                    "dragged": "#fff44336",
                                                                    "empty": "#fff44336",
                                                                    "error": "#fff44336",
                                                                    "focused": "#fff44336",
                                                                    "hovered": "#fff44336",
                                                                    "pressed": "#fff44336",
                                                                    "scrolledUnder": "#fff44336",
                                                                    "selected": "#fff44336"
                                                                }
                                                            },
                                                            "child": {
                                                                "type": "text",
                                                                "args": {
                                                                    "text": "Voltar"
                                                                }
                                                            }
                                                        }
                                                    }
                                                ]
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}